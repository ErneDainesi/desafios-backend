<div class="container-form">
	<h1>Ingrese Producto</h1>
	<form action="/productos" method="POST" enctype="application/x-www-form-urlencoded">
		<input type="text" name="title" placeholder="Titulo">
		<input type="number" name="price" placeholder="Precio">
		<input type="url" name="thumbnail" placeholder="Url Imagen">
		<button type="submit" id="products-submit">Enviar</button>
	</form>
</div>

